{% macro Team01(obj) %}
    <a href="javascript:void(0)" class="card card-team-01">
        <div class="card-image">
            <img src="{{obj.img}}" alt="card">
        </div>
        <div class="card-texts">
            <h3 class="card-title heading-dark">{{obj.title}}</h3>
            <p class="text-dark">{{obj.text}}</p>
        </div>
    </a>
{% endmacro %}
{% macro Team02(obj) %}
    <div class="card-team-02">
        <div class="card-team-02--img">
            <img src="{{obj.img}}" alt="team-img">
        </div>
        <div class="card-team-02__body">
            <a href="" class="card-team-02__body--category text-bule">{{obj.category}}</a>
            <h3 class="card-team-02__body--name">{{obj.name}}</h3>
            <span class="heading-dark card-team-02__body--specialist">{{obj.specialist}}</span>
            <p class="heading-dark card-team-02__body--info">{{obj.text}}</p>
            <div class="card-team-02__body--button-group">
                <span class="text {{obj.colorClass}}">{{obj.date}}</span>
                <a class="btn-2 btn-primary btn-primary-hvr" href="#">Book Today</a>
            </div>
        </div>
        <div class="card-team-02--tag">
            <div class="ratting">
                <i class="fas fa-star"></i>
                <span>4.5</span>
            </div>
        </div>
    </div>
{% endmacro %}
