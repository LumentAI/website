{% macro Feature01(obj) %}
    <div class="card-feature-01">
        <div class="card-feature-01--img">
            <img src="{{obj.img}}" alt="testimonil-img" class="w-100">
            <h6 class="subject {{obj.colorClass}}">
                {{obj.subject}}
            </h6>
        </div>
        <div class="card-feature-01__body">
            <h6 class="card-feature-01__body--title">
                {{obj.title | safe }}
            </h6>
            <div class="card-feature-01__body--ratting">
                <i class="fas fa-star"></i>
                <i class="fas fa-star"></i>
                <i class="fas fa-star"></i>
                <i class="fas fa-star"></i>
                <i class="fas fa-star"></i>
            </div>
            <div class="card-feature-01__body--button-group">
                <h6 class="card-feature-01__body--button-group-price">${{obj.minPrice}}
                    <del>${{obj.disPrice}}</del>
                </h6>
                <a href="#" class="btn btn-primary btn-primary -hvr">Enroll Now</a>
            </div>
        </div>
    </div>
{% endmacro %}

{% macro BlogCard01(obj) %}
    <div class="blog-card-01">
        <div class="blog-card-01--img">
            <img src="{{obj.img}}" alt="blog-img" class="w-100">
        </div>
        <div class="blog-card-01__body">
            <div class="blog-card-01__body--meta">
                <div class="admmin">
                    <img src="./image/blog/admin.svg" class="make-it-inline" alt="blog-image"/>
                    <span>{{obj.meta}}</span>
                </div>
                <div class="date">
                    <img src="./image/blog/clender.svg" class="make-it-inline" alt="blog-image"/>
                    <span>{{obj.date}}</span>
                </div>
            </div>
            <div class="blog-card-01__body--content">
                <h6 class="title">{{obj.title}}</h6>
                <p class="text-dark text">{{obj.text}}</p>
                <a class="more-btn" href="#">Read More <img src="./image/blog/arrow.svg" class="make-it-inline" alt="icon"></a>
            </div>
        </div>
    </div>
{% endmacro %}