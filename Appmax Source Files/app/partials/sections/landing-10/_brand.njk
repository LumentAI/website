{% macro render(args) %}
    {% set brands = ["./image/home-10/brand-1.png", "./image/home-10/brand-2.png", "./image/home-10/brand-3.png", "./image/home-10/brand-4.png", "./image/home-10/brand-5.png"] %}
    <!-- START BRAND SECTION -->
    <div class="brand-section-02 brand-section-modifi--04">
        <div class="container">
            <div class="brand-section-02__wrapper">
                {% for item in brands %}
                    <div class="single-brand">
                        <img src="{{item}}" alt="brand">
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
    <!-- END BRAND SECTION -->
{% endmacro %}