{% macro render(args) %}
    {% import "components/Cards/pricings.njk" as PricingCard %}
    <div class="pricing-section-03">
        <div class="pricing-section-03__shape" data-aos="fade-left" data-aos-delay="500" data-aos-duration="1000">
            <img src="./image/home-6/content-shape.svg" alt="shape">
        </div>
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6 text-center">
                    <div class="section-title pricing-section-03__title">
                        <h6 class="subtitle text-secondary  mb-20">Pricing & Plans </h6>
                        <h2 class="title">We have exclusive plan <br class="d-none d-xs-block"> in our pricing</h2>
                        <p class="text-dark">An enim nullam tempor sapien gravida donec enim <br class="d-none d-xs-block">
                        ipsum porta justo integena gna at pretium </p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-12 col-md-9 col-xs-11">
                    <div class="row pricing-sider three-row-mobile-slider mobile-slider">
                        <div class="col-lg-4" data-aos="fade-up" data-aos-delay="200" data-aos-duration="1000">
                            {{PricingCard.Three({
                        colorClass:"text-green",
                        plan:"Starter Plan",
                        price:"5",
                        listOne:"10 GB Disk Space",
                        listTwo:"1 Year Support",
                        listThree:"500 Queries",
                        listFour:"Basic Statistics",
                        listFive:"Free Custom Domain"
                        })}}
                        </div>
                        <div class="col-lg-4" data-aos="fade-up" data-aos-delay="400" data-aos-duration="1000">
                            {{PricingCard.Three({
                        activeClass:"active",
                        colorClass:"text-bule",
                        plan:"Basic Plan",
                        price:"45",
                        listOne:"500 GB Disk Space",
                        listTwo:"5 Year Support",
                        listThree:"1000 Queries",
                        listFour:"Basic Statistics",
                        listFive:"Free Custom Domain"
                        })}}
                        </div>
                        <div class="col-lg-4" data-aos="fade-up" data-aos-delay="600" data-aos-duration="1000">
                            {{PricingCard.Three({
                        colorClass:"text-red",
                        plan:"Premium Plan",
                        price:"100",
                        listOne:"800 GB Disk Space",
                        listTwo:"Unlimitedr Support",
                        listThree:"Unlimited Queries",
                        listFour:"Full Statistics",
                        listFive:"Free Custom Domain"
                        })}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endmacro %}